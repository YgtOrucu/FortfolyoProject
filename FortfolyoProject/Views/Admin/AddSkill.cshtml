
@{
    ViewData["Title"] = "AddSkill";
    Layout = "~/Views/Admin/IndexLayout.cshtml";
}

<h3>AddSkill</h3>
<br />

<form asp-controller="Admin" asp-action="AddSkill" method="post">

    <label for="Title">Başlık</label>
    <input class="form-control" style="width:50%" id="Title" name="Title" />
    <br />
    <label for="Values">Value</label>
    <input id="Values" name="Values" class="form-control" style="width:50%" type="number" />
    <br />
    <button type="submit" class="btn btn-success">
        Add
    </button>
</form>
